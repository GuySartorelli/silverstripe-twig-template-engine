{
    "name": "silverstripe/twig-template-engine",
    "type": "silverstripe-vendormodule",
    "description": "The SilverStripe twig templating engine",
    "license": "BSD-3-Clause",
    "keywords": [
        "silverstripe",
        "twig",
        "template",
        "parser"
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "silverstripe/framework": "^4@dev",
        "silverstripe/vendor-plugin": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\Twig\\": "src/"
        }
    },
    "include-path": [
        "src/"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
